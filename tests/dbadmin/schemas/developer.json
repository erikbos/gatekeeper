{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "developerId": {
            "type": "string"
        },
        "status": {
            "type": "string",
            "enum": ["active", "inactive"]
        },
        "apps": {
            "type": "array",
            "items": [{
                "type": "string"
            }]
        },
        "attributes": {
            "type": "array",
            "items": {
                "$ref": "attribute.json"
            }
        },
        "email": {
            "type": "string"
        },
        "userName": {
            "type": "string"
        },
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        },
        "createdAt": {
            "type": "integer"
        },
        "createdBy": {
            "type": "string"
        },
        "lastmodifiedAt": {
            "type": "integer"
        },
        "lastmodifiedBy": {
            "type": "string"
        }
    },
    "required": [
        "developerId",
        "status",
        "apps",
        "attributes",
        "email",
        "userName",
        "firstName",
        "lastName",
        "createdAt",
        "createdBy",
        "lastModifiedAt",
        "lastModifiedBy"
    ]
}