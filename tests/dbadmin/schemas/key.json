{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "apiProducts": {
            "type": "array",
            "items": {}
        },
        "consumerKey": {
            "type": "string"
        },
        "consumerSecret": {
            "type": "string"
        },
        "expiresAt": {
            "type": "integer"
        },
        "issuedAt": {
            "type": "integer"
        },
        "scopes": {
            "type": "array",
            "items": {}
        },
        "status": {
            "type": "string",
            "enum": ["approved", "revoked"]
        }
    },
    "required": [
        "consumerKey",
        "consumerSecret",
        "expiresAt",
        "issuedAt",
        "status"
    ]
}