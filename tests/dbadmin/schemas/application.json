{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "apiProducts": {
            "type": "array",
            "items": {}
        },
        "appFamily": {
            "type": "string"
        },
        "appId": {
            "type": "string"
        },
        "attributes": {
            "type": "array",
            "items": {
                "$ref": "attribute.json"
            }
        },
        "callbackUrl": {
            "type": "string"
        },
        "createdAt": {
            "type": "integer"
        },
        "credentials": {
            "type": "array",
            "items": {
                "$ref": "credential.json"
            }
        },
        "developerId": {
            "type": "string"
        },
        "keyExpiresIn": {
            "type": "integer"
        },
        "lastModifiedAt": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "scopes": {
            "type": "array",
            "items": {}
        },
        "status": {
            "type": "string"
        }
    },
    "required": [
        "appId",
        "developerId",
        "createdAt",
        "lastModifiedAt"
    ]
}