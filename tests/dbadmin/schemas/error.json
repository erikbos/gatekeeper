{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "code": {
            "anyOf": [{
                    "type": "integer",
                    "minimum": 100,
                    "maximum": 599
                },
                {
                    "type": "string"
                }
            ]
        },
        "message": {
            "type": "string"
        }
    },
    "required": [
        "code",
        "message"
    ]
}