# Example REST calls for cluster


### Get all virtualhosts
GET http://{{hostname}}/v1/virtualhosts


### Create virtualhost
POST http://{{hostname}}/v1/virtualhosts HTTP/1.1
content-type: application/json

{
    "name": "example_80",
    "displayName": "Example Inc.",
    "virtualHosts": [
         "www.example.com"
    ],
    "port": 80,
    "organizationName": "petstore",
    "routeGroup": "routes_80"
}

### Update a virtual host
POST http://{{hostname}}/v1/virtualhosts/example_80 HTTP/1.1
content-type: application/json

{
    "name": "example_80",
    "displayName": "Example Inc.",
    "virtualHosts": [
        "www.example.com"
    ],
    "port": 80,
    "routeGroup": "routes_80",
    "policies": "geoiplookup",
    "organizationName": "petstore",
    "attributes": [
        {
            "name": "HTTPProtocol",
            "value": "HTTP/1/.1"
        }
    ]
}

###
POST http://{{hostname}}/v1/virtualhosts
content-type: application/json

{
    "name": "example_443_1",
    "displayName": "example secure",
    "port": 443
}

###
POST http://{{hostname}}/v1/virtualhosts/example_443_1
content-type: application/json

{
    "name": "example_443_1",
    "displayName": "example secure",
    "virtualHosts": [
        "www.example.com",
        "test.com"
    ],
    "port": 443,
    "routeGroup": "routes_443",
    "policies": "geoiplookup,checkapikey",
    "organizationName": "petstore",
    "attributes": [
        {
            "name": "HTTPProtocol",
            "value": "HTTP/2"
        },
        {
            "name": "TLSCertificate",
            "value": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----"
        },
        {
            "name": "TLSCertificateKey",
            "value": "-----BEGIN RSA PRIVATE KEY-----\\n-----END RSA PRIVATE KEY-----"
        },
        {
            "name": "TLSCipherSuites",
            "value": "[ECDHE-RSA-CHACHA20-POLY1305|ECDHE-RSA-AES256-GCM-SHA384|ECDHE-RSA-AES128-GCM-SHA256]"
        },
        {
            "name": "TLSMinimumVersion",
            "value": "TLSv1_2"
        }
    ]
}

